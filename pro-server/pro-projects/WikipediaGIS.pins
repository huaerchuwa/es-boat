([WikipediaGIS_Class20000] of  CloFunction

	(source "(let [mp (into {} hm)\n       sel (selection mp \"feedURL\")]\n  (if (seq sel)\n      (if-let [resp (call-geonames-rss (str (first sel)))]\n          (let [inss (map #(map-into-inst % (crin \"GeoRSSItem\")) resp)\n                 cins (filter-coords-exist inss \"geo:lat\" \"geo:long\")\n                 crds (coords-from-instances cins \"geo:lat\" \"geo:long\")\n                 bbx (if (seq crds) (bbx-of-list crds))]\n             (ssvs inst \"georss_items\" inss)\n             (ssvs inst \"bbx\" inss)) )))")
	(title "submit-rss [hm inst]"))

([WikipediaGIS_Class20005] of  CloFunction

	(source ";; returns [lat lon]\n(let [[w s e n] (seq (svs bbx-ins \"wsen\"))]\n  [(float (/ (+ s n) 2)) (float (/ (+ w e) 2))])")
	(title "get-bbx-center [bbx-ins]"))

([WikipediaGIS_Class20006] of  CloFunction

	(source ";; filter instances with nonempty coords\n(filter #(and (sv % slat) (sv % slon)) list)")
	(title "filter-coords-exist [inss slat slon]"))

([WikipediaGIS_Class20007] of  CloFunction

	(source ";; Collect coords from list of instances for given slot names\n(map #(vector (sv % slat) (sv % slon)) list)")
	(title "coords-from-instances [cins slat slon]"))

([WikipediaGIS_Class20008] of  CloFunction

	(source ";; crds = ([lat lon]..)\n([crds title]\n  ;; return instance of BBX class\n  (if-let [wsen (bbx-of-list  crds)]\n    (let [ins (crin \"BBX\")]\n       (ssvs ins \"wsen\" wsen))))\n([crds]\n  ;; return [west south east north] or nil\n  (try\n    (letfn [(apl [max first]\n	(apply max (map read-string (map first list))))]\n      [(apl min second) (apl min first) (apl max second) (apl max first)])\n  (catch Exception e\n     (println [:bbx-of-list list :exception e])\n     nil)))")
	(title "bbx-of-list"))

([WikipediaGIS_Class20014] of  BBX

	(title "Helsinki")
	(wsen 24.5 59.9 25.5 60.5))

([WikipediaGIS_Class30023] of  CloFunction

	(source "(let [mp (into {} hm)\n       url (or (mp \"wikipediaUrl\") (mp \"guid\"))\n       wss (cls-instances \"WebSite\")]\n  (let [ws (if (seq wss)\n                  (first wss)\n                  (crin \"WebSite\"))]\n    (ssv ws \"webpage\" url)\n    (doto (.show *prj* ws)\n       (.pack)\n       (.show)) ))")
	(title "open-site [hm inst]"))

([WikipediaGIS_Class40028] of  CloFunction

	(source "(let [mp (into {} hm)\n       max (mp \"max-responses\")\n       lang (request-lang (mp \"language\"))\n       radius-km (mp \"radius-km\")\n       [lat lon] (get-bbx-center (mp \"bbx\"))]\n  (if-let [resp (call-wiki-nearby lat lon radius-km max lang)]\n     (ssvs inst \"responses\" (map article-from-map resp)) ))")
	(title "submit-nearby [hm inst]"))

